                  <action name="Create PPPInfo properties file" id="2837" customizedId="" beanClass="com.install4j.runtime.beans.actions.control.RunScriptAction" enabled="true" commentSet="false" comment="" actionElevationType="elevated" rollbackBarrier="false" rollbackBarrierExitCode="0" multiExec="false" failureStrategy="1" errorMessage="">
                    <serializedBean>
                      <java class="java.beans.XMLDecoder">
                        <object class="com.install4j.runtime.beans.actions.control.RunScriptAction">
                          <void property="script">
                            <object class="com.install4j.api.beans.ScriptProperty">
                              <void property="value">
                                <string>import java.io.File;
import java.io.FileInputStream;
import java.io.FileOutputStream;
import java.io.IOException;
import java.io.InputStream;
import java.io.OutputStream;
import java.nio.file.Files;
import java.nio.file.Path;
import java.nio.file.Paths;
import java.util.Properties;

String PPP_INFO_PROPERTIES = "PPPInfo.properties";
String BY_INSTALLER_UPDATE_OR_RESTORATION = "By_Installer_Update_or_Restoration";
String SOFTWARE_UPDATE_TYPE_VALUE_INSTALLER = "Installer";
String SOFTWARE_UPDATE_FROM_PPP_VERSION = "From_PPP_version";
String SOFTWARE_UPDATE_TO_PPP_VERSION = "To_PPP_version";

try {	   
    String propertiesPath = (String)context.getVariable("sys.installationDir") + "/log/" + PPP_INFO_PROPERTIES; 
    Properties prop = new Properties();
    if (new File(propertiesPath).exists()) {
        InputStream input = new FileInputStream(propertiesPath);
        prop.load(input);
        input.close();
        
        prop.setProperty(SOFTWARE_UPDATE_FROM_PPP_VERSION, prop.getProperty(SOFTWARE_UPDATE_TO_PPP_VERSION)); 
    }else {
        String logFolderPath = (String)context.getVariable("sys.installationDir") + "/log";        
        Path pLogFolderPath = Paths.get(logFolderPath);
        if (Files.notExists(pLogFolderPath)) {
             Files.createDirectories(pLogFolderPath);  
             Path pPath = Paths.get(propertiesPath);
             Files.createFile(pPath);                  
        }    
        
        prop.setProperty(BY_INSTALLER_UPDATE_OR_RESTORATION, SOFTWARE_UPDATE_TYPE_VALUE_INSTALLER);
        prop.setProperty(SOFTWARE_UPDATE_FROM_PPP_VERSION, "");     
    }
   
    OutputStream output = new FileOutputStream(propertiesPath);
    prop.store(output, null);
    output.close();            
    return true;
        
} catch (IOException ex) {
    return false;
}</string>
                              </void>
                            </object>
                          </void>
                        </object>
                      </java>
                    </serializedBean>
