                  <action name="SetLanguage" id="2286" customizedId="" beanClass="com.install4j.runtime.beans.actions.control.RunScriptAction" enabled="true" commentSet="false" comment="" actionElevationType="elevated" rollbackBarrier="false" rollbackBarrierExitCode="0" multiExec="false" failureStrategy="1" errorMessage="">
                    <serializedBean>
                      <java class="java.beans.XMLDecoder">
                        <object class="com.install4j.runtime.beans.actions.control.RunScriptAction">
                          <void property="script">
                            <object class="com.install4j.api.beans.ScriptProperty">
                              <void property="value">
                                <string>import java.io.*;

Boolean result = false;

try {
    String installationDir = (String)context.getVariable("sys.installationDir");
    //Util.showMessage(installationDir);

    //String language = context.getVariable("language").toString();
    String languageId = context.getVariable("sys.languageId").toString().toLowerCase().trim();
    //Util.showMessage(languageId);

    String lang = "en_US";
    if(languageId.equals("de")){
        lang = "de_DE";
    }else if(languageId.equals("ja")){
        lang = "ja_JP";
    }else if(languageId.equals("cs")){
        lang = "cs_CZ";
    }else if(languageId.equals("fr")){
        lang = "fr_FR";
    }else if(languageId.equals("es")){
        lang = "es_ES";
    }else if(languageId.equals("pl")){
        lang = "pl";
    }else if(languageId.equals("zh_cn")){
        lang = "zh_CN";
    }else if(languageId.equals("zh_tw")){
        lang = "zh_TW";
    }else if(languageId.equals("ru")){
        lang = "ru";
    }
    //Util.showMessage(lang);

    // The name of the file to open.
    String filePath = installationDir + File.separator + "PowerPanel Personal.app" + File.separator + "Contents" + File.separator + "MacOS" + File.separator + "i18n" + File.separator + "language.txt";

    //Util.showMessage(filePath);

    // Assume default encoding.
    FileWriter fileWriter = new FileWriter(filePath);

    // Always wrap FileWriter in BufferedWriter.
    BufferedWriter bufferedWriter = new BufferedWriter(fileWriter);

    // append a newline character.
    bufferedWriter.write(lang);

    // Always close files.
    bufferedWriter.close();

    result = true;
}
catch(IOException ex) {
    Util.showMessage(ex.getMessage());
}

return result;
</string>
